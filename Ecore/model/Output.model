<?xml version="1.0" encoding="ASCII"?>
<hoho:config xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:hoho="http://ezdevops2.0com" name="mon projet">
  <agent type="docker"/>
  <stages xsi:type="hoho:Cloning" url="https://github.com/monprojettestt.git" credentialID="personal-cloning-key" branch="main"/>
  <stages xsi:type="hoho:Tests" shell="mvn test" classestotest="ClassesToTestRetrieval.java, CredentialsRetrieval.java, JenkinsCredentialRetrieval.java, Manip1.java"/>
